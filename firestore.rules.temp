rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    // ⚠️ TEMPORARY RULES FOR SEEDING - DEPLOY ONLY DURING SEEDING!
    // ⚠️ REVERT TO SECURE RULES AFTER SEEDING COMPLETES!
    
    // TEMPORARY: Allow all writes for seeding (REVERT AFTER!)
    match /{document=**} {
      allow read: if true;
      allow write: if true;  // ⚠️ INSECURE - ONLY FOR SEEDING
    }
  }
}
